div>\r\n          <div class=\"mb-3\">\r\n            <label for=\"inputPassword\" class=\"form-label\">密碼</label>\r\n            <input type=\"password\" id=\"inputPassword\" class=\"form-control\"\r\n              placeholder=\"請輸入密碼\" required v-model=\"user.password\"\r\n            />\r\n          </div>\r\n          <div class=\"d-grid mt-4\">\r\n            <router-link to=\"/\" class=\"btn btn-secondary btn-lg\" type=\"submit\">回到首頁</router-link>\r\n          </div>\r\n          <div class=\"d-grid mt-4\">\r\n            <button class=\"btn btn-primary btn-lg\" type=\"submit\">登入</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ToastMessage from '@/mixins/ToastMessage'\r\nexport default {\r\n  data () {\r\n    return {\r\n      user: {\r\n        username: '',\r\n        password: ''\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    signIn () {\r\n      const api = `${process.env.VUE_APP_API}admin/signin`\r\n      this.$http.post(api, this.user)\r\n        .then((res) => {\r\n          if (res.data.success) {\r\n            const { token, expired } = res.data\r\n            document.cookie = `hexToken=${token}; expires=${new Date(expired)}`\r\n            this.$router.push('/dashboard/products')\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          this.showToast('error', '登入失敗，請確認帳號密碼是否正確')\r\n          console.error('登入錯誤：', err)\r\n        })\r\n    }\r\n  },\r\n  mixins: [ToastMessage]\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login-page {\r\n  background: linear-gradient(135deg, #e0eafc, #cfdef3);\r\n}\r\n</style>\r\n"],"mappings":";AAkCA,OAAOA,YAAW,MAAO,uBAAsB;AAC/C,eAAe;EACbC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE;MACZ;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,MAAKA,CAAA,EAAK;MACR,MAAMC,GAAE,GAAI,GAAGC,OAAO,CAACC,GAAG,CAACC,WAAW,cAAa;MACnD,IAAI,CAACC,KAAK,CAACC,IAAI,CAACL,GAAG,EAAE,IAAI,CAACL,IAAI,EAC3BW,IAAI,CAAEC,G